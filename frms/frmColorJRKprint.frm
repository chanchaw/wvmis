Private Sub sumall()
     Dim rs As New RecordSet
    Dim a As Double
    Dim b As Double
    Dim c As Double
    Dim d As Double
    Dim e As String
    Dim f As String
    Dim k As String
    If rss.RecordCount <= 0 Then
        Exit Sub
    End If
    a = 0
    b = 0
    c = 0
    d = 0
    Set rs = rss.Clone
    rs.MoveFirst
    Do While Not rs.EOF
        a = a + IIf(IsNull(rs!sum1), 0, rs!sum1)
        b = b + IIf(IsNull(rs!Sum2), 0, rs!Sum2)
        c = c + IIf(IsNull(rs!Sum3), 0, rs!Sum3)
        d = d + IIf(IsNull(rs!sum4), 0, rs!sum4)
        rs.movenext
    Loop
    rs.MoveFirst
    e = Format(b, "0.0")
    f = Format(c, "0.0")
    k = Format(d, "0.0")
    TDBGrid1.Columns("rowindex").FooterText = "合计"
    TDBGrid1.Columns("sum1").FooterText = "" & a & ""
    TDBGrid1.Columns("sum2").FooterText = "" & e & ""
    TDBGrid1.Columns("sum3").FooterText = "" & f & ""
    TDBGrid1.Columns("sum4").FooterText = "" & k & ""
End Sub

Private Sub setGridShow()
    Dim cls1 As New clsGridShow
    With cls1
        .ObjectID = "11S054"
        .InitClass TDBGrid1, 3
        .ShowGridFormat
    End With
End Sub
Private Sub setGridStyle()
    Dim i As Long
    Dim strSQL As String
    Dim dWidth As Integer
    Dim szFieldName As String
    
    For i = 0 To TDBGrid1.Columns.Count - 1
        If TDBGrid1.Columns(i).width > 0 Then
            If TDBGrid1.Columns(i).Visible = True Then
                szFieldName = TDBGrid1.Columns(i).DataField
                dWidth = TDBGrid1.Columns(i).width
                strSQL = "update G_BLSField set B_GridWidth='" & dWidth & "' where B_ObjectID='11S054' and B_FieldName='" & szFieldName & "'"
                Gm.cnnTool.cnn.Execute strSQL
            End If
        End If
    Next
End Sub

Private Sub Pri()
    If TDBGrid1.ApproxCount <= 0 Then
        Exit Sub
    End If
    Dim sql As String
    Dim rs As New RecordSet
    sql = "exec P_Report_GetDetailFormal '" & rss!B_DTRK & "'"
    Debug.Print sql
    rs.Open sql, Gm.cnnTool.cnn, adOpenKeyset, adLockPessimistic
    Dim frm1 As New frmModBLRPreviewOriColor
    Set frm1.RecordSet = rs.Clone
       If rs!B_EDP = "001" Then
                frm1.ObjectID = "22B072"
       Else
             frm1.ObjectID = "22B141"
       End If
    frm1.Show vbModal
    Unload frm1
End Sub
